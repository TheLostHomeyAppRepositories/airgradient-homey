{
	"type": "number",
	"title": {
		"en": "PM1"
	},
	"units": {
		"en": "µg/m³"
	  },
	"decimals": 3,
	"getable": true,
	"setable": false,
	"insights": true,
	"uiComponent": "sensor",
	"icon": "./assets/measure_pm1.svg",
    "$flow": {
        "triggers": [
            {
                "id": "measure_pm1_changed",
                "title": {
                    "en": "The PM1 value has changed"
                },
                "tokens": [
                    {
                        "name": "$id",
                        "title": "$title",
                        "type": "$type",
                        "example": 2
                    }
                ]
            }
        ]
    }
}