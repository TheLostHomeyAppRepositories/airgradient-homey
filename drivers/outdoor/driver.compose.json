{
  "name": {
    "en": "Outdoor Monitor",
    "nl": "Buitenmonitor",
    "da": "Udendørs skærm",
    "de": "Außenmonitor",
    "es": "Monitor Exterior",
    "fr": "Moniteur Extérieur",
    "it": "Monitor Esterno",
    "no": "Utemonitor",
    "sv": "Utomhusmonitor",
    "pl": "Monitor Zewnętrzny",
    "ru": "Наружный монитор",
    "ko": "실외 모니터"
  },
  "class": "sensor",
  "capabilities": [
    "measure_pm1",
    "measure_pm25",
    "measure_pm10",
    "measure_co2",
    "measure_pm03_cnt",
    "measure_temperature",
    "measure_humidity",
    "measure_voc",
    "measure_voc_idx",
    "measure_nox",
    "measure_nox_idx"
  ],
  "platforms": [
    "local"
  ],
  "connectivity": [
    "lan"
  ],
  "images": {
    "small": "{{driverAssetsPath}}/images/small.png",
    "large": "{{driverAssetsPath}}/images/large.png"
  },
  "discovery": "airgradient",
  "pair": [
    {
      "id": "list_devices",
      "template": "list_devices",
      "options": {
        "singular": true
      },
      "navigation": {
        "next": "add_devices"
      }
    },
    {
      "id": "device_ip",
      "navigation": {
        "next": "add_devices"
      }
    },
    {
      "id": "add_devices",
      "template": "add_devices"
    }
  ]
}